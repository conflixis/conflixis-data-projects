# Environment Variables for Healthcare COI Analytics
# Copy this file to .env and fill in your actual values

# Google Cloud Platform
GCP_SERVICE_ACCOUNT_KEY='{"type": "service_account", "project_id": "your-project", ...}'

# BigQuery Settings (optional - can use CONFIG.yaml instead)
BQ_PROJECT_ID=data-analytics-389803
BQ_DATASET=conflixis_agent

# API Keys (if needed for additional data sources)
# CMS_API_KEY=your_cms_api_key_here
# FDA_API_KEY=your_fda_api_key_here

# Database Connections (if applicable)
# SNOWFLAKE_ACCOUNT=your_account
# SNOWFLAKE_USER=your_user
# SNOWFLAKE_PASSWORD=your_password
# SNOWFLAKE_WAREHOUSE=your_warehouse
# SNOWFLAKE_DATABASE=your_database
# SNOWFLAKE_SCHEMA=your_schema

# Email Configuration (optional)
# SMTP_USERNAME=your_email@example.com
# SMTP_PASSWORD=your_email_password

# Logging
LOG_LEVEL=INFO

# Performance
MAX_WORKERS=4
CHUNK_SIZE=10000

# Debug Mode
DEBUG=false